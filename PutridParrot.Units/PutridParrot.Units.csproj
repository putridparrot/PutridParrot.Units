<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;netstandard2.0;netstandard2.1;net48</TargetFrameworks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
	<Company>PutridParrot</Company>
	<Authors>Mark Timmings</Authors>
	<Description>Unit conversion functions for C#</Description>
	<Version>1.0.10-alpha</Version>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/putridparrot/PutridParrot.Units.git</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageIcon>PutridParrot.Units.png</PackageIcon>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageTags>Unit converters</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\PutridParrot.Units.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
